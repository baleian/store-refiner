    "build:prd": "react-scripts -r dotenv/config build dotenv_config_path=./config/production.env",
    "test": "react-scripts -r dotenv/config test dotenv_config_path=./config/test.env",
    "eject": "#warning! react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
